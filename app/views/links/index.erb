
<html>
<h1> Hello <%= @email %> </h1>
<h1> Here is the list of your bookmarks</h1>

<ul id='links'>
  <% @links.each do |link| %>
    <li> Name: <%=link.link_name%>
        URL: <%=link.link_url%>
        Tags: <% link.tags.each { |tag| %>
          <%= "#{tag.tag_name}" %>
          <% } %>
    </li>
  <% end %>
</ul>

</html>
